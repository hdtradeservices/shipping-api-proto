{
  "swagger": "2.0",
  "info": {
    "title": "Shipping Integration Stores API",
    "version": "v2"
  },
  "tags": [
    {
      "name": "StoresService"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v2/shipping/stores": {
      "get": {
        "summary": "List retrieves a single listing by its ID",
        "operationId": "StoresService_List",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/listing_apiListResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "StoresService"
        ]
      },
      "post": {
        "summary": "Update creates a new store",
        "operationId": "StoresService_Create",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/listing_apiCreateResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "StoresService"
        ]
      }
    },
    "/v2/shipping/stores/{storeId}": {
      "delete": {
        "summary": "Delete can be used to delete existing stores",
        "operationId": "StoresService_Delete",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/listing_apiDeleteResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "storeId",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "StoresService"
        ]
      },
      "put": {
        "summary": "Update can be used to update existing stores",
        "operationId": "StoresService_Update",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/listing_apiUpdateResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "storeId",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "StoresService"
        ]
      }
    }
  },
  "definitions": {
    "listing_apiCreateResponse": {
      "type": "object"
    },
    "listing_apiDeleteResponse": {
      "type": "object"
    },
    "listing_apiListResponse": {
      "type": "object",
      "properties": {
        "stores": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/listing_apiStore"
          }
        }
      }
    },
    "listing_apiStore": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "displayName": {
          "type": "string"
        }
      },
      "title": "Store contains an ID and display name for a store"
    },
    "listing_apiUpdateResponse": {
      "type": "object"
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  }
